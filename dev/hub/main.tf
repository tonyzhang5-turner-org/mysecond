module "asg-elb" {
  source                               = "git::ssh://git@github.com/turnercode/terraform.git//aws/asg-elb-http-https-internal?ref=v2.3.5"
  aws_account                          = "${var.aws_account}"
  aws_named_profile                    = "${var.aws_named_profile}"
  name                                 = "${var.name}"
  products                             = "${var.products}"
  environment                          = "${var.environment}"
  customer                             = "${var.customer}"
  conftag                              = "${var.conftag}"
  owner                                = "${var.owner}"
  team                                 = "${var.team}"
  description                          = "${var.description}"
  contact-email                        = "${var.contact-email}"
  asg_desired_instances                = "${var.asg_desired_instances}"
  asg_max_instances                    = "${var.asg_max_instances}"
  asg_min_instances                    = "${var.asg_min_instances}"
  aws_instance_type                    = "${var.aws_instance_type}"
  package_size                         = "${var.package_size}"
  aws_ami                              = "${var.aws_ami}"
  lvm                                  = "${var.lvm}"
  mkfs                                 = "${var.mkfs}"
  elb_listener_ssl_arn                 = "${var.elb_listener_ssl_arn}"
  elb_listener_instance_port           = "${var.elb_listener_instance_port}"
  elb_listener_lb_port                 = "${var.elb_listener_lb_port}"
  elb_health_check_target              = "${var.elb_health_check_target}"
  elb_health_check_healthy_threshold   = "${var.elb_health_check_healthy_threshold}"
  elb_health_check_unhealthy_threshold = "${var.elb_health_check_unhealthy_threshold}"
  elb_health_check_timeout             = "${var.elb_health_check_timeout}"
  elb_health_check_interval            = "${var.elb_health_check_interval}"
  billing-customer                     = "${var.billing-customer}"
  asg_specific_security_groups         = "${var.asg_specific_security_groups}"
  lb_specific_security_groups          = "${var.lb_specific_security_groups}"
  lb_specific_zone_identifiers         = "${var.lb_specific_zone_identifiers}"
  lb_ssl_listener_policy_name          = "${var.lb_ssl_listener_policy_name}"
}
